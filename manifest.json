{
    "manifest_version": 3,
    "name": "GPTkeys",
    "description": "Adds keyboard shortcuts to chatGPT",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": ["https://chat.openai.com/*"],
            "js": ["dist/content.js"]
        }
    ],
    "background": {
        "service_worker": "dist/background.js"
    }
}